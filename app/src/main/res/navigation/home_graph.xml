<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_graph"
    app:startDestination="@id/homeFragment2">

    <fragment
        android:id="@+id/homeFragment2"
        android:name="com.example.gotour.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment2_to_exploreFragment"
            app:destination="@id/exploreFragment" />
        <action
            android:id="@+id/action_homeFragment2_to_mapFragment"
            app:destination="@id/mapFragment" />
        <action
            android:id="@+id/action_homeFragment2_to_notificationFragment"
            app:destination="@id/notificationFragment" />
        <action
            android:id="@+id/action_homeFragment2_to_massageFragment"
            app:destination="@id/massageFragment" />
        <action
            android:id="@+id/action_homeFragment2_to_settingFragment"
            app:destination="@id/settingFragment" />
        <action
            android:id="@+id/action_homeFragment2_to_paymentMethodFragment"
            app:destination="@id/paymentMethodFragment" />
    </fragment>
    <fragment
        android:id="@+id/exploreFragment"
        android:name="com.example.gotour.ExploreFragment"
        android:label="fragment_explore"
        tools:layout="@layout/fragment_explore" >
        <action
            android:id="@+id/action_exploreFragment_to_explore2"
            app:destination="@id/explore2" />
        <action
            android:id="@+id/action_exploreFragment_to_paymentMethodFragment"
            app:destination="@id/paymentMethodFragment" />
    </fragment>
    <fragment
        android:id="@+id/mapFragment"
        android:name="com.example.gotour.MapFragment"
        android:label="fragment_map"
        tools:layout="@layout/fragment_map" />
    <fragment
        android:id="@+id/massageFragment"
        android:name="com.example.gotour.MassageFragment"
        android:label="fragment_massage"
        tools:layout="@layout/fragment_massage" />
    <fragment
        android:id="@+id/settingFragment"
        android:name="com.example.gotour.SettingFragment"
        android:label="fragment_setting"
        tools:layout="@layout/fragment_setting" >
        <action
            android:id="@+id/action_settingFragment_to_openSettingFragment"
            app:destination="@id/openSettingFragment" />
    </fragment>
    <fragment
        android:id="@+id/notificationFragment"
        android:name="com.example.gotour.NotificationFragment"
        android:label="fragment_notification"
        tools:layout="@layout/fragment_notification" />
    <fragment
        android:id="@+id/openSettingFragment"
        android:name="com.example.gotour.OpenSettingFragment"
        android:label="fragment_open_setting"
        tools:layout="@layout/fragment_open_setting" >
        <action
            android:id="@+id/action_openSettingFragment_to_paymentMethodFragment"
            app:destination="@id/paymentMethodFragment" />
    </fragment>
    <fragment
        android:id="@+id/explore2"
        android:name="com.example.gotour.Explore2"
        android:label="fragment_explore2"
        tools:layout="@layout/fragment_explore2" >
        <action
            android:id="@+id/action_explore2_to_explore3"
            app:destination="@id/explore3" />
        <action
            android:id="@+id/action_explore2_to_paymentMethodFragment"
            app:destination="@id/paymentMethodFragment" />
    </fragment>
    <fragment
        android:id="@+id/explore3"
        android:name="com.example.gotour.Explore3"
        android:label="fragment_explore3"
        tools:layout="@layout/fragment_explore3" >
        <action
            android:id="@+id/action_explore3_to_exploreaboutFragment"
            app:destination="@id/exploreaboutFragment" />
        <action
            android:id="@+id/action_explore3_to_paymentMethodFragment"
            app:destination="@id/paymentMethodFragment" />
    </fragment>
    <fragment
        android:id="@+id/exploreaboutFragment"
        android:name="com.example.gotour.ExploreaboutFragment"
        android:label="fragment_exploreabout"
        tools:layout="@layout/fragment_exploreabout" >
        <action
            android:id="@+id/action_exploreaboutFragment_to_paymentMethodFragment"
            app:destination="@id/paymentMethodFragment" />
    </fragment>
    <fragment
        android:id="@+id/paymentMethodFragment"
        android:name="com.example.gotour.PaymentMethodFragment"
        android:label="fragment_payment_method"
        tools:layout="@layout/fragment_payment_method" >
        <action
            android:id="@+id/action_paymentMethodFragment_to_payment2Fragment"
            app:destination="@id/payment2Fragment" />
    </fragment>
    <fragment
        android:id="@+id/payment2Fragment"
        android:name="com.example.gotour.Payment2Fragment"
        android:label="fragment_payment2"
        tools:layout="@layout/fragment_payment2" />
</navigation>